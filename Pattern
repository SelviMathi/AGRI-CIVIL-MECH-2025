import java.util.Scanner;
public class cond2 {
   public static void main(String[] args) {
    Scanner kbd=new Scanner(System.in);
    int n=kbd.nextInt();
    char ch='A';
    for(int row=1;row<=n;row++)
    {
      for(int col=1;col<=n;col++)
      System.out.print(ch++);
      System.out.println();
    }
  }
}

